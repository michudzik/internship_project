<div class="container">
  <h1>Users</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th colspan="2">Change role</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <%= render 'user', user: user %>
      <% end %>
    </tbody>
  </table>
</div>